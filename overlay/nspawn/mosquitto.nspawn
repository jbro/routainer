[Exec]
Ephemeral=true
Hostname=mosquitto
LinkJournal=auto
PrivateUsers=0
NotifyReady=yes

[Files]
BindReadOnly=/data/routainer/containers/mosquitto:/opt/routainer
BindReadOnly=/data/routainer/containers/mosquitto/network:/etc/systemd/network
Bind=/data/mosquitto/:/opt/mosquitto/

[Network]
Bridge=linknet
