[Exec]
Ephemeral=true
Hostname=managementnet
LinkJournal=auto
PrivateUsers=0
NotifyReady=yes
ResolvConf=copy-host

[Files]
BindReadOnly=/data/routainer/containers/managementnet:/opt/routainer
BindReadOnly=/data/routainer/containers/managementnet/network:/etc/systemd/network

[Network]
Interface=enp3s0
Bridge=linknet
